<!DOCTYPE html>
<html>
	
  <%= render 'layouts/htmlhead' %>
  
  <body>
    
    <%= render 'layouts/bodyhead'%>
    
    <div class="container">
      	<% flash.each do |message_type, message| %>
		  <div class="alert alert-<%= message_type %>"><%= message %></div>
		<% end %>
     	<%= yield %>
      	<%= render 'layouts/footer' %>
      	<%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>